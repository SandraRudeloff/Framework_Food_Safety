  author = {Wellman, Allison and Bazaco, Michael C and Blessington, Tyann and Pightling, Arthur and Dwarka, Asha and Hintz, Leslie and Wise, Matthew E. and Gieraltowski, Laura and Conrad, Amanda and Nguyen, Thai-An and Hise, Kelley and Viazis, Stelios and Beal, Jennifer},
