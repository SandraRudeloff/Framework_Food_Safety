@article{uelze.etal2020,
  title = {Typing Methods Based on Whole Genome Sequencing Data},
  author = {Uelze, Laura and Gr{\"u}tzke, Josephine and Borowiak, Maria and Hammerl, Jens Andre and Juraschek, Katharina and Deneke, Carlus and Tausch, Simon H. and Malorny, Burkhard},
