@article{briz-redon.etal2019,
  title = {Estimating the Occurrence of Traffic Accidents near School Locations: {{A}} Case Study from {{Valencia}} ({{Spain}}) Including Several Approaches},
  shorttitle = {Estimating the Occurrence of Traffic Accidents near School Locations},
  author = {{Briz-Red{\'o}n}, {\'A}lvaro and {Mart{\'i}nez-Ruiz}, Francisco and Montes, Francisco},
