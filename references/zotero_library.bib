@article{briz-redon.etal2022,
  title = {Modeling the {{Influence}} of {{Places}} on {{Crime Risk Through}} a {{Non-Linear Effects Model}}: A {{Comparison}} with {{Risk Terrain Modeling}}},
  shorttitle = {Modeling the {{Influence}} of {{Places}} on {{Crime Risk Through}} a {{Non-Linear Effects Model}}},
  author = {{Briz-Red{\'o}n}, {\'A}lvaro and Mateu, Jorge and Montes, Francisco},
  year = {2022},
