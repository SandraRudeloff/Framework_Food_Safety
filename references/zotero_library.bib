@article{kim.etal2019a,
  title = {Influence of Prior {{pH}} and Thermal Stresses on Thermal Tolerance of Foodborne Pathogens},
  author = {Kim, Chyer and Bushlaibi, Mariam and Alrefaei, Rana and Ndegwa, Eunice and Kaseloo, Paul and Wynn, Crystal},
  year = {2019},
  month = may,
  journal = {Food Science \& Nutrition},
  volume = {7},
