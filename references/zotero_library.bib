@mastersthesis{rudeloff2020,
  title = {Identifying the {{Source}} of {{Foodborne Diseases}} Using {{Data}} on {{Imports}} into the {{United States}}},
  author = {Rudeloff, Sandra},
  year = {2020},
  month = jul,
  address = {Hamburg, Germany},
  abstract = {The complexity of the international food trade network is rising. Consequently, importing countries are increasingly vulnerable to the spread of large-scale foodborne diseases. New approaches such as computational traceback models are continuously evolving to support investigators in the rapid identification of contaminated food item and their location source intending to prevent the further proliferation of the disease. However, little attention is dedicated to the traceback of foodborne diseases associated with imported food items, mostly due to the difficulty of tracing through the convoluted international food supply network. This thesis aims to fill this gap by applying a model integrating food item and source detection to data on ocean imports into the United States. The developed method is based upon the recently introduced likelihood-based approach, that has already been adopted in a different setting in the context of tracing of foodborne diseases. The theoretical key contribution of this thesis is the extension of the approach and its application to real data as an integrated item and source detection model. The overall research goal is to identify a contaminated product and its source by comparing product import patterns with the spatial distribution of a given outbreak. The mathematical model built for this purpose represents the core element of the method. The applicability of the model is assessed using artificially generated outbreak scenarios that are based upon the import pattern of the respective simulated contaminated product. The predictive power is defined as its capability of correctly identifying the simulated contaminated product or including it in a shortlist of the most suspicious products given the artificially generated outbreak scenario. The results show that the performance of the model increases steeply with a rising number of outbreak cases. Especially, if a shortlist of five suspicious products instead of one single product is provided, the model can contribute to practice by guiding laboratory testing already at an early stage. Thus, traceback of foodborne disease outbreaks associated with imported food items can be accelerated and further proliferation prevented.},
  langid = {american},
  school = {K{\"u}hne Logistics University},
  file = {/Users/sandi/Zotero/storage/NG9SJSLZ/Masterthesis Sandra Rudeloff.pdf}
}

