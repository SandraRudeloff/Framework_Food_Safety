@article{tao.etal2023,
  title = {Epidemiological {{Data Mining}} for {{Assisting}} with {{Foodborne Outbreak Investigation}}},
  author = {Tao, Dandan and Zhang, Dongyu and Hu, Ruofan and Rundensteiner, Elke and Feng, Hao},
  year = {2023},
