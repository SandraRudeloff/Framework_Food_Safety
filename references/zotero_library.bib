@article{white.etal2022a,
  title = {Using {{Outbreak Data}} for {{Hypothesis Generation}}: {{A Vehicle Prediction Tool}} for {{Disease Outbreaks Caused}} by {{Salmonella}} and {{Shiga Toxin-Producing Escherichia}} Coli},
  shorttitle = {Using {{Outbreak Data}} for {{Hypothesis Generation}}},
  author = {White, Alice E. and Jackson, Conner and Kisselburgh, Hannah and Ledbetter, Caroline and Scallan Walter, Elaine},
  year = {2022},
  month = apr,
  journal = {Foodborne Pathogens and Disease},
  volume = {19},
